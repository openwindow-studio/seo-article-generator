# Environment variables for SEO Article Generator

# Node Environment
NODE_ENV=development

# API Configuration
API_BASE_URL=http://localhost:3000
API_TIMEOUT=30000

# File Upload Limits
MAX_ARTICLES_PER_REQUEST=50
MAX_WORD_COUNT=5000

# Feature Flags
ENABLE_DOWNLOAD=true
ENABLE_BULK_OPERATIONS=true
ENABLE_REALTIME_PREVIEW=true

# Analytics (Optional)
# GOOGLE_ANALYTICS_ID=G-XXXXXXXXXX
# VERCEL_ANALYTICS=true